package kh.java.func;

public class VariableTest {
	
	public void test1() {
		boolean bool = true;
		char ch = 'A';
		int num1 = 100;
		System.out.println(bool);
		System.out.println(ch);
		System.out.println(num1);
	}
	
	public void test2() {
		String name = "전한기";
		int age = 26;
		String adress = "신정동";
		System.out.println(name + age + adress);
		
	}

}

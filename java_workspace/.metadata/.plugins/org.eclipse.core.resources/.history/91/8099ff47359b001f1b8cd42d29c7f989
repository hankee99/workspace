package kh.java;
import java.util.Scanner;
import java.lang.Math;

public class HelloJava {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("입력하시오: ");
		String a = sc.next();
		int b = sc.nextInt();
		
		
		System.out.println("Hello Java\n" + a + b);
		sc.close();
	}

}

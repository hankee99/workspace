package excc;


import java.util.ArrayList;


public class EXCCCC {

	public static void main(String[] args) {
		ArrayList<Integer> arr = new ArrayList<>();
		int[] arr2 = {1,2,3};
		arr.add(1);
		arr.add(2);
		arr.add(3);
		arr.add(4);
		
		System.out.println(arr.toString());
		System.out.println(arr2);
		
		
		
	}

}
